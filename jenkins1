pipeline {
    agent none
    stages{
        stage('fetch-code'){
            steps {
                git branch: 'main', url: 'https://github.com/pratikshachougule/login-data.git'
            }
        }
        stage('build-image'){
            steps{
                sh 'docker build -t ditiss2:v1 .'
            }
        }
    }
}
