pipeline{
    agent none
    stages{
         stage('docker-stop'){
            steps{
            sh 'docker stop ditiss2'
            sh 'docker rm ditiss2'
            }
        }
        stage('deploy-image'){
            steps{
                sh 'docker run --name ditiss2 -p 9500:80 -d ditiss2:v1'
            }
        }
    }
}
